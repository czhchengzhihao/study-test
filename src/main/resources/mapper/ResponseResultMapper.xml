<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.dao.ResponseResultDao">

    <insert id="addResponseResult" parameterType="com.study.pojo.ResponseResult">
        INSERT INTO response_result (success, message, entity)
        VALUES (#{success}, #{message}, #{entity})
    </insert>
</mapper>